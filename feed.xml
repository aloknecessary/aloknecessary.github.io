layout: null
---

<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

  <title>Aloknecessary â€“ Architecture Blog</title>
  <link href="{{ site.url }}{{ site.baseurl }}/feed.xml" rel="self"/>
  <link href="{{ site.url }}{{ site.baseurl }}/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.url }}{{ site.baseurl }}/</id>

  {% assign blogs = site.blogs | sort: 'date' | reverse %}
  {% for post in blogs limit:25 %}

  
  <entry>
    <title>{{ post.title }}</title>
    <link href="{{ site.url }}{{ post.url }}"/>
    <id>{{ site.url }}{{ post.url }}</id>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <summary type="html">{{ post.excerpt | xml_escape }}</summary>
  </entry>
  {% endfor %}

</feed>
